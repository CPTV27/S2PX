        "@vitejs/plugin-react": "^5.0.4",
        "clsx": "^2.1.1",
        "cookie-parser": "^1.4.7",
        "cors": "^2.8.5",
        "crypto-random-string": "^5.0.0",
        "dotenv": "^17.3.1",
        "drizzle-orm": "^0.38.0",
        "drizzle-zod": "^0.7.0",
        "express": "^4.21.0",
        "firebase": "^12.9.0",
        "firebase-admin": "^13.6.1",
        "framer-motion": "^12.34.2",
        "lucide-react": "^0.546.0",
        "motion": "^12.23.24",
        "multer": "^1.4.5-lts.1",
        "nodemailer": "^8.0.1",
        "pdfkit": "^0.17.2",
        "pg": "^8.13.0",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "react-hook-form": "^7.54.0",
        "react-markdown": "^10.1.0",
        "react-router-dom": "^7.13.0",
        "recharts": "^3.7.0",
        "rehype-slug": "^6.0.0",
        "remark-gfm": "^4.0.1",
        "tailwind-merge": "^3.5.0",
        "vite": "^6.2.0",
        "zod": "^3.24.0"
    },
