{
    "name": "s2px",
    "private": true,
    "version": "1.0.0",
    "type": "module",
    "scripts": {
        "dev": "concurrently --names client,server --prefix-colors cyan,yellow \"vite\" \"tsx watch server/index.ts\"",
        "dev:client": "vite",
        "dev:server": "tsx watch server/index.ts",
        "build": "tsc -b && vite build",
        "preview": "vite preview",
        "clean": "rm -rf dist",
        "lint": "tsc --noEmit",
        "db:push": "drizzle-kit push",
        "db:generate": "drizzle-kit generate",
        "db:migrate": "drizzle-kit migrate",
        "db:studio": "drizzle-kit studio",
        "test": "vitest run",
        "test:watch": "vitest"
    },
    "dependencies": {
        "@google-cloud/storage": "^7.14.0",
        "@google/genai": "^1.29.0",
